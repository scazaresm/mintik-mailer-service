{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "new-cap": "off",
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "max-len": "off"
    }
}